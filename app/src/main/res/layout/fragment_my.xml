<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.FurnitureShare.ui.fragment.MyFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#EAEAEA"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/rl_head"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:fitsSystemWindows="true"
            android:background="@mipmap/mybackground">

            <com.example.FurnitureShare.view.CircleImageView
                android:id="@+id/iv_head"
                android:layout_width="55dp"
                android:layout_height="55dp"
                android:layout_centerInParent="true"
                android:src="@mipmap/nologin"/>

            <TextView
                android:id="@+id/tv_name"
                android:layout_below="@+id/iv_head"
                android:layout_marginTop="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:text="点击登录"
                android:textSize="15sp"/>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_myorder"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#ffffff"
            android:layout_below="@+id/rl_head"
            android:layout_alignParentStart="true">


            <TextView
                android:layout_marginLeft="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="我的订单"
                android:textSize="18sp"
                android:textColor="#484848"
                android:layout_centerVertical="true"/>


            <ImageView
                android:id="@+id/iv_myorder"
                android:layout_width="9dp"
                android:layout_height="16dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="15dp"
                android:src="@drawable/common_icon_enter"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentBottom="true"
                android:background="#dadada"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"/>
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/ll"
            android:background="#ffffff"
            android:layout_width="match_parent"
            android:layout_below="@+id/rl_myorder"
            android:layout_height="75dp"
            android:orientation="horizontal">

            <RelativeLayout

                android:id="@+id/rl_obligation"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <ImageView
                    android:id="@+id/iv_obligtion"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12dp"
                    android:layout_centerHorizontal="true"
                    android:src="@mipmap/obligtion"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_below="@+id/iv_obligtion"
                    android:layout_marginTop="5dp"
                    android:text="待付款"
                    android:textSize="14sp"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_sendgoods"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <ImageView
                    android:id="@+id/iv_sendgoods"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12dp"
                    android:layout_centerHorizontal="true"
                    android:src="@mipmap/sendgoods"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_below="@+id/iv_sendgoods"
                    android:layout_marginTop="5dp"
                    android:text="待发货"
                    android:textSize="14sp"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_beenshipped"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <ImageView
                    android:id="@+id/iv_beenshipped"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12dp"
                    android:layout_centerHorizontal="true"
                    android:src="@mipmap/beenshipped"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_below="@+id/iv_beenshipped"
                    android:layout_marginTop="5dp"
                    android:text="已发货"
                    android:textSize="14sp"/>

            </RelativeLayout>
        </LinearLayout>

        <RelativeLayout
            android:id="@+id/rl_contract"
            android:layout_below="@+id/ll"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_alignParentRight="true"
            android:layout_alignParentLeft="true"
            android:layout_marginTop="8dp"
            android:background="#ffffff">

            <ImageView
                android:id="@+id/iv1"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:src="@mipmap/contract"/>

            <TextView
                android:layout_toRightOf="@+id/iv1"
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="我的合同"
                android:textSize="14sp"
                android:textColor="#484848"
                android:layout_centerVertical="true"/>


            <ImageView
                android:id="@+id/iv_warehouse"
                android:layout_width="9dp"
                android:layout_height="16dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="15dp"
                android:src="@drawable/common_icon_enter"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentBottom="true"
                android:background="#dadada"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"/>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_certification"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_alignParentRight="true"
            android:layout_alignParentLeft="true"
            android:background="#ffffff">

            <ImageView
                android:id="@+id/iv2"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:src="@mipmap/certification"/>

            <TextView
                android:layout_toRightOf="@+id/iv2"
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="信用认证"
                android:textSize="14sp"
                android:textColor="#484848"
                android:layout_centerVertical="true"/>


            <ImageView
                android:id="@+id/iv_integral"
                android:layout_width="9dp"
                android:layout_height="16dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="15dp"
                android:src="@drawable/common_icon_enter"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentBottom="true"
                android:background="#dadada"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"/>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_address"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_alignParentRight="true"
            android:layout_alignParentLeft="true"
            android:background="#ffffff">

            <ImageView
                android:id="@+id/iv3"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:src="@mipmap/address"/>

            <TextView
                android:layout_toRightOf="@+id/iv3"
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="地址管理"
                android:textSize="14sp"
                android:textColor="#484848"
                android:layout_centerVertical="true"/>


            <ImageView
                android:id="@+id/iv_address"
                android:layout_width="9dp"
                android:layout_height="16dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="15dp"
                android:src="@drawable/common_icon_enter"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentBottom="true"
                android:background="#dadada"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"/>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_warranty"
            android:layout_below="@+id/rl_address"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_alignParentRight="true"
            android:layout_alignParentLeft="true"
            android:background="#ffffff">

            <ImageView
                android:id="@+id/iv4"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:src="@mipmap/warranty"/>

            <TextView
                android:layout_toRightOf="@+id/iv4"
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="预约保修"
                android:textSize="14sp"
                android:textColor="#484848"
                android:layout_centerVertical="true"/>


            <ImageView
                android:id="@+id/iv_goodsshelves"
                android:layout_width="9dp"
                android:layout_height="16dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="15dp"
                android:src="@drawable/common_icon_enter"/>


        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_set"
            android:layout_below="@+id/rl_openshop"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_alignParentRight="true"
            android:layout_alignParentLeft="true"
            android:layout_marginTop="8dp"
            android:background="#ffffff">

            <ImageView
                android:id="@+id/iv5"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:src="@mipmap/set"/>

            <TextView
                android:layout_toRightOf="@+id/iv5"
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="系统设置"
                android:textSize="14sp"
                android:textColor="#484848"
                android:layout_centerVertical="true"/>


            <ImageView
                android:id="@+id/iv_set"
                android:layout_width="9dp"
                android:layout_height="16dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="15dp"
                android:src="@drawable/common_icon_enter"/>


        </RelativeLayout>

    </LinearLayout>


</RelativeLayout>
